---
import { getCollection } from "astro:content";
import Baz from "../layouts/Baz.astro";



const allBits = await getCollection("bits");
const pageTitle = "bits"
---

<Baz pageTitle={pageTitle}>
  <p>All bits</p>
  {
      allBits.map((post) => (
      <a href={`${post.id}`}>{post.data.title}</a>
      ))
    }
</Baz>

